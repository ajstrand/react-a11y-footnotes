{
  "name": "react-a11y-footnotes",
  "version": "0.7.0",
  "description": "A reusable preact implementation of accessible footnotes.",
  "homepage": "https://github.com/ajstrand/react-a11y-footnotes",
  "license": "MIT",
  "main": "dist/react-footnotes-a11y.es.js",
  "type": "module",
  "keywords": [
    "footnotes",
    "footnote",
    "accessibility",
    "a11y",
    "react",
    "preact",
    "component"
  ],
  "author": "Kitty Giraudel (https://kittygiraudel.com)",
  "files": [
    "dist/react-footnotes-a11y.es.js",
    "dist/styles.css",
    "dist/index.d.ts"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "dev": "npm run peer && vite",
    "preview": "vite preview",
    "build": "vite build",
    "test": "npm run peer && vitest",
    "coverage": "vitest run --coverage",
    "peer": "npm info . peerDependencies --json  | command sed 's/[\\{\\},]//g ; s/: /@/g' | xargs npm i --no-save"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@preact/preset-vite": "^2.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/preact": "^3.2.3",
    "jsdom":"^21.1.1",
    "vite": "^4.2.0",
    "vitest": "^0.27.3"
  },
  "peerDependencies": {
    "preact": ">=10.0.0",
    "prop-types": "^15.8.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ajstrand/react-a11y-footnotes/"
  }
}
